<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/cardview_dark_background"
    tools:context=".MainActivity">

    <!-- Tecla 8  -->
   <!-- Tecla 2 -->
   <!-- Tecla 1 -->

   <Button
       android:id="@+id/button11"
       android:layout_width="wrap_content"
       android:layout_height="100dp"
       android:layout_marginStart="8dp"
       android:layout_marginTop="8dp"
       android:layout_marginEnd="8dp"
       android:text="@string/tecl08"
       app:layout_constraintEnd_toStartOf="@+id/button12"
       app:layout_constraintStart_toEndOf="@+id/button10"
       app:layout_constraintTop_toBottomOf="@+id/button2" />

   <Button
       android:id="@+id/button10"
       android:layout_width="wrap_content"
       android:layout_height="100dp"
       android:layout_marginStart="8dp"
       android:layout_marginTop="8dp"
       android:text="@string/tecl07"
       app:layout_constraintEnd_toStartOf="@+id/button11"
       app:layout_constraintStart_toStartOf="parent"
       app:layout_constraintTop_toBottomOf="@+id/button2" />

   <Button
       android:id="@+id/button5"
       android:layout_width="wrap_content"
       android:layout_height="100dp"
       android:layout_marginStart="8dp"
       android:layout_marginTop="2dp"
       android:text="@string/tecl03"
       app:layout_constraintStart_toEndOf="@+id/button6"
       app:layout_constraintTop_toBottomOf="@+id/button9" />

   <Button
       android:id="@+id/button8"
       android:layout_width="wrap_content"
       android:layout_height="100dp"
       android:layout_marginStart="8dp"
       android:layout_marginTop="2dp"
       android:text="@string/tecl05"
       app:layout_constraintStart_toEndOf="@+id/button7"
       app:layout_constraintTop_toBottomOf="@+id/button11" />

   <Button
       android:id="@+id/button7"
       android:layout_width="wrap_content"
       android:layout_height="100dp"
       android:layout_marginStart="8dp"
       android:layout_marginTop="2dp"
       android:layout_marginEnd="8dp"
       android:text="@string/tecl04"
       app:layout_constraintEnd_toStartOf="@+id/button8"
       app:layout_constraintStart_toStartOf="parent"
       app:layout_constraintTop_toBottomOf="@+id/button10" />

   <Button
       android:id="@+id/button12"
       android:layout_width="wrap_content"
       android:layout_height="100dp"
       android:layout_marginStart="8dp"
       android:layout_marginTop="8dp"
       android:text="@string/tecl09"
       app:layout_constraintStart_toEndOf="@+id/button11"
       app:layout_constraintTop_toBottomOf="@+id/button" />

   <Button
       android:id="@+id/button9"
       android:layout_width="wrap_content"
       android:layout_height="100dp"
       android:layout_marginStart="8dp"
       android:layout_marginTop="2dp"
       android:text="@string/tecl06"
       app:layout_constraintStart_toEndOf="@+id/button8"
       app:layout_constraintTop_toBottomOf="@+id/button12" />

   <!-- Tecla 0 -->
   <!-- Tecla 5 -->
   <!-- Tecla 4 -->

   <Button
       android:id="@+id/button6"
       android:layout_width="wrap_content"
       android:layout_height="100dp"
       android:layout_marginStart="8dp"
       android:layout_marginTop="2dp"
       android:text="@string/tecl02"
       app:layout_constraintStart_toEndOf="@+id/button4"
       app:layout_constraintTop_toBottomOf="@+id/button8" />

   <Button
       android:id="@+id/button4"
       android:layout_width="wrap_content"
       android:layout_height="100dp"
       android:layout_marginStart="8dp"
       android:layout_marginTop="2dp"
       android:text="@string/tecl01"
       app:layout_constraintStart_toStartOf="parent"
       app:layout_constraintTop_toBottomOf="@+id/button7" />

   <Button
       android:id="@+id/button3"
       android:layout_width="184dp"
       android:layout_height="100dp"
       android:layout_marginStart="8dp"
       android:layout_marginTop="2dp"
       android:text="@string/tecl00"
       app:layout_constraintStart_toStartOf="parent"
       app:layout_constraintTop_toBottomOf="@+id/button4"
       tools:ignore="DuplicateSpeakableTextCheck" />

   <Button
       android:id="@+id/button2"
       android:layout_width="184dp"
       android:layout_height="100dp"
       android:layout_marginStart="8dp"
       android:layout_marginTop="28dp"
       android:backgroundTint="@color/teal_700"
       android:focusable="auto"
       android:onClick="Holabtn"
       android:text="@string/resultado"
       app:layout_constraintStart_toStartOf="parent"
       app:layout_constraintTop_toBottomOf="@+id/layout1"
       tools:ignore="UsingOnClickInXml,TextContrastCheck" />

   <Button
       android:id="@+id/button"
       android:layout_width="wrap_content"
       android:layout_height="100dp"
       android:layout_marginStart="8dp"
       android:layout_marginTop="28dp"
       android:backgroundTint="@color/azuloperador"
       android:onClick="Mundobtn"
       android:text="@string/mult"
       app:layout_constraintStart_toEndOf="@+id/button2"
       app:layout_constraintTop_toBottomOf="@+id/layout1"
       tools:ignore="UsingOnClickInXml" />

   <TextView
       android:id="@+id/layout1"
       android:layout_width="389dp"
       android:layout_height="58dp"
       android:layout_marginLeft="8dp"
       android:layout_marginTop="10dp"
       android:layout_marginRight="8dp"
       android:background="@color/black"
       android:backgroundTint="@color/cardview_shadow_start_color"
       android:duplicateParentState="false"
       android:shadowColor="@color/purple_700"
       android:text="@string/disp"
       android:textAlignment="textEnd"
       android:textColor="@color/white"
       android:textSize="42sp"
       app:layout_constraintBottom_toBottomOf="parent"
       app:layout_constraintHorizontal_bias="0.428"
       app:layout_constraintLeft_toLeftOf="parent"
       app:layout_constraintRight_toRightOf="parent"
       app:layout_constraintTop_toTopOf="parent"
       app:layout_constraintVertical_bias="0.038" />

   <androidx.constraintlayout.widget.Barrier
       android:id="@+id/barrier2"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       app:barrierDirection="top"
       app:constraint_referenced_ids="button6,button,button4,button3,button8,button7,button2,button12,button5,button11,button9,button10"
       tools:layout_editor_absoluteY="110dp" />

   <androidx.constraintlayout.widget.Group
       android:id="@+id/group"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content" />

   <Button
       android:id="@+id/button13"
       android:layout_width="88dp"
       android:layout_height="100dp"
       android:layout_marginStart="8dp"
       android:layout_marginTop="2dp"
       android:text="@string/decimal"
       app:layout_constraintStart_toEndOf="@+id/button3"
       app:layout_constraintTop_toBottomOf="@+id/button5" />

   <Button
       android:id="@+id/button14"
       android:layout_width="0dp"
       android:layout_height="100dp"
       android:layout_marginStart="8dp"
       android:layout_marginTop="28dp"
       android:layout_marginEnd="8dp"
       android:backgroundTint="@color/azuloperador"
       android:text="@string/div"
       app:layout_constraintEnd_toEndOf="parent"
       app:layout_constraintStart_toEndOf="@+id/button"
       app:layout_constraintTop_toBottomOf="@+id/layout1" />

   <Button
       android:id="@+id/button15"
       android:layout_width="0dp"
       android:layout_height="100dp"
       android:layout_marginStart="8dp"
       android:layout_marginTop="8dp"
       android:layout_marginEnd="8dp"
       android:backgroundTint="@color/azuloperador"
       android:text="@string/resta"
       app:layout_constraintEnd_toEndOf="parent"
       app:layout_constraintStart_toEndOf="@+id/button12"
       app:layout_constraintTop_toBottomOf="@+id/button14" />

   <Button
       android:id="@+id/button16"
       android:layout_width="0dp"
       android:layout_height="203dp"
       android:layout_marginStart="8dp"
       android:layout_marginTop="2dp"
       android:layout_marginEnd="8dp"
       android:backgroundTint="@color/azuloperador"
       android:text="@string/suma"
       app:layout_constraintEnd_toEndOf="parent"
       app:layout_constraintStart_toEndOf="@+id/button9"
       app:layout_constraintTop_toBottomOf="@+id/button15" />

   <Button
       android:id="@+id/button17"
       android:layout_width="0dp"
       android:layout_height="100dp"
       android:layout_marginStart="8dp"
       android:layout_marginTop="2dp"
       android:layout_marginEnd="8dp"
       android:backgroundTint="@color/design_default_color_error"
       android:onClick="teclaAC"
       android:text="@string/ac"
       app:layout_constraintEnd_toEndOf="parent"
       app:layout_constraintStart_toEndOf="@+id/button5"
       app:layout_constraintTop_toBottomOf="@+id/button16"
       tools:ignore="UsingOnClickInXml,TextContrastCheck" />

   <TextView
       android:id="@+id/sublayout"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_marginTop="5dp"
       android:layout_marginEnd="16dp"
       android:text="@string/tecl00"
       android:textColor="@color/teal_200"
       app:layout_constraintEnd_toEndOf="parent"
       app:layout_constraintTop_toBottomOf="@+id/layout1" />

</androidx.constraintlayout.widget.ConstraintLayout>